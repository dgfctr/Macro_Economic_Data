  data_sym == "HB"                ? "Per 1000 People"       :
  data_sym == "ICUB"              ? "Per 1000 People"       :
  na
//

// Functions

f_dig(x) =>
    w = 10e17
    q = 10e14
    t = 10e11
    b = 10e8
    m = 10e5
    k = 10e2
    sonuc =
      x >= w or x <= -w ? str.tostring(x/q, "#.##") + " Kentilyon" :
      x >= q or x <= -q ? str.tostring(x/q, "#.##") + " Katrilyon" :
      x >= t or x <= -t ? str.tostring(x/t, "#.##") + " Trilyon"   :
      x >= b or x <= -b ? str.tostring(x/b, "#.##") + " Milyar"    :
      x >= m or x <= -m ? str.tostring(x/m, "#.##") + " Milyon"    : 
      x >= k or x <= -k ? str.tostring(x/k, "#.##") + " Bin"       : 
      str.tostring(x, "#.##")
//

f_style(x) => 
      x == "Çizgi"          ? plot.style_line             : 
      x == "İşaretli Çizgi" ? plot.style_line             : 
      x == "Histogram"      ? plot.style_histogram        : 
      x == "Sütun"          ? plot.style_columns          : 
      x == "Alan"           ? plot.style_area             : 
      x == "Nokta"          ? plot.style_circles          : 
      x == "Step"           ? plot.style_stepline_diamond : 
      na
//

f_size(x) =>
      x == "Küçük"  ? size.small  : 
      x == 'Normal' ? size.normal : 
      x == "Büyük"  ? size.large  :
      na 
//

// Inputs
gr_k    = "KONFİGÜRASYON"
pb      = input.string(title="Para Birimi", options=["USD", "EUR", "TRY"], defval="USD", group=gr_k)
stil    = input.string(title="Çizim Tercihi", options=[
  "Çizgi", "İşaretli Çizgi", "Histogram", "Sütun", "Alan", "Step", "Nokta"], defval="İşaretli Çizgi", group=gr_k)
size    = input.string(title="Yazı Boyutu", options=["Küçük", "Normal", "Büyük"], defval="Normal", group=gr_k)
renk_d  = input.color(title="Renk", defval=#2962ff, group=gr_k)
tablo_g = input.bool(title="Bilgi Paneli", defval=true, group=gr_k)
renk_b  = #686868

// Calcs
sembol = 
  data_sym == "NAEXKP01TRQ652S" or 
  data_sym == "INTDSRTRM193N"   or 
  data_sym == "TURCPIALLMINMEI" ? 
  "FRED:" + data_sym : 
  "ECONOMICS:" + ulke_sym + data_sym
r           = request.security(sembol, "", close, ignore_invalid_symbol=true, currency=pb)
sonuc       = f_dig(r)
sonuc_      = str.tostring(r, "###,###,###,###,###,###,###.##")
son_degisim = str.format_time(ta.valuewhen(r!=r[1], time, 0), "dd.MM.yyyy", 'GMT+3')
birim       = birim_ == "USD" or birim_ == "TRY" ? pb : birim_
kaynak      = ulke == "Türkiye" ? kaynak_kurum : "..."
description = request.security(sembol, "", syminfo.description) 

// Info Panel
var table tablo = table.new(position=position.top_right, columns=3, rows=15)
f_tb(x, y, c, s, t) =>
    table.cell(tablo, x-1, y-1, t, text_color=c, text_halign=s=="l" ? text.align_left : text.align_right, text_size=f_size(size))
if barstate.islast and tablo_g
    i = "" + "\n\nÜlke\n\nData\n\nBirim\n\nSonuç\n\n" + "" + "\n\nDescription\n\nSon Değişim\n\nKaynak\n\nSembol"
    ii = "" + "\n\n : \n\n : \n\n : \n\n : \n\n" + "" + "\n\n : \n\n : \n\n : \n\n : "
    iii = "" + "\n\n" + ulke + "\n\n" + data + "\n\n" + birim + "\n\n" + sonuc + "\n\n[" + sonuc_ + "]\n\n" + 
      description + " (" + data_sym + ")\n\n" + son_degisim + "\n\n" + kaynak + "\n\n" + sembol
    f_tb(1, 1, renk_b, "", i)
    f_tb(2, 1, renk_b, "", ii)
    f_tb(3, 1, renk_d, "l", iii)
//

// Plots
plot(r, "Sonuç", renk_d, stil == "Nokta" ? 3 : 1, f_style(stil), editable=false)
plot(stil == "İşaretli Çizgi" ? r : na, "", renk_d, 3, plot.style_circles, editable=false)

// Bitti
plotshape(barstate.isfirst, title="@ dg_factor", location=location.top, color=#13172200, editable=false)

